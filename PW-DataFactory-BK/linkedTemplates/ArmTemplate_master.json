{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "PW-DataFactory-BK"
		},
		"Azure_SQLDB_to_PWAzureTraining_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Azure_SQLDB_to_PWAzureTraining'"
		},
		"BlobStorage_azurestorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobStorage_azurestorage'"
		},
		"OnPrem_SQL_Server_Connection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPrem_SQL_Server_Connection'"
		},
		"OnPrem_SQL_Server_Connection_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPrem_SQL_Server_Connection'"
		},
		"ls_blob_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_sa'"
		},
		"OnPrem_SQL_Server_Connection_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-1EEQMSF\\bek"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "PW-DataFactory-BK_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Azure_SQLDB_to_PWAzureTraining_connectionString": {
						"value": "[parameters('Azure_SQLDB_to_PWAzureTraining_connectionString')]"
					},
					"BlobStorage_azurestorage_connectionString": {
						"value": "[parameters('BlobStorage_azurestorage_connectionString')]"
					},
					"ls_blob_sa_connectionString": {
						"value": "[parameters('ls_blob_sa_connectionString')]"
					},
					"OnPrem_SQL_Server_Connection_connectionString": {
						"value": "[parameters('OnPrem_SQL_Server_Connection_connectionString')]"
					},
					"OnPrem_SQL_Server_Connection_password": {
						"value": "[parameters('OnPrem_SQL_Server_Connection_password')]"
					},
					"OnPrem_SQL_Server_Connection_properties_typeProperties_userName": {
						"value": "[parameters('OnPrem_SQL_Server_Connection_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}